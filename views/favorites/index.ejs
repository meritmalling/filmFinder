<h2> favorites </h2>

<div class="col-md-6 col-md-offset-3">
          <% myFavorites.forEach(function(favorites){ %>
                    <div class="well">
                              <a href="/show/<%= favorites.imdbId %>"><b><%= favorites.title %></a><br>
                              <b>Year: </b><%= favorites.year %><br>
                              <a href="/favorites/show/<%= favorites.imdbId %>">Add Comment <i class="glyphicon glyphicon-comment"></i></a>
                               <a href="/favorites/<%= favorites.imdbId %>/tags/new">Add Tag <i class="glyphicon glyphicon-tag"></i></a>
                    </div>
          <% }); %>
</div>