<h1>add a new tag</h1>

<div class="col-sm-4 col-sm-offset-4">
<form action="/favorites/<%= favoriteId %>/tags" method="POST" role="form">
          <div class="form-group">
                    <input type="text" class="form-control" id="tagName" placeholder="#hashtag" name="tagName">
          </div>
          <button type="submit" class="btn btn-primary">save tag</button>
</form>
</div>
